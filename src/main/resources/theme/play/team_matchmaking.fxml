<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0"
      prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1"
      fx:controller="com.faforever.client.teammatchmaking.TeamMatchmakingController">
   <children>
      <HBox maxHeight="Infinity" maxWidth="Infinity" spacing="10.0">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="card"
                  HBox.hgrow="ALWAYS">

               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
               </padding>
               <children>
                  <Label styleClass="h2" text="Party"/>
                  <HBox spacing="10.0">
                     <children>
                        <JFXButton text="Invite player" defaultButton="true"/>
                        <JFXButton disable="true" layoutX="10.0" layoutY="10.0" text="Leave party" cancelButton="true"/>
                     </children>
                  </HBox>
                  <JFXListView fx:id="playerListView" styleClass="party-player-list-view" mouseTransparent="true"
                               focusTraversable="false"/>
               </children>
               <opaqueInsets>
                  <Insets/>
               </opaqueInsets>
            </VBox>
            <VBox minWidth="350.0" styleClass="card">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
               </padding>
               <children>
                  <Label styleClass="h2" text="Available queues"/>
                  <HBox spacing="10.0">
                     <children>
                        <JFXButton disable="true" text="Leave queue" cancelButton="true"/>
                     </children>
                  </HBox>
                  <JFXListView/>
               </children>
               <opaqueInsets>
                  <Insets/>
               </opaqueInsets>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
         </padding>
      </HBox>
   </children>
</VBox>
